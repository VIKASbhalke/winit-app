<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".HomeActivity">

    <ImageButton
        android:id="@+id/faq"
        android:layout_width="308dp"
        android:layout_height="77dp"
        android:layout_marginTop="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/appfaqbutton2"
        android:contentDescription="@string/faq" />

    <ImageButton
        android:id="@+id/schedule"
        android:layout_width="308dp"
        android:layout_height="77dp"
        android:layout_marginTop="28dp"
        android:contentDescription="@string/schedule"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/faq"
        app:srcCompat="@drawable/appcalendarbutton" />

    <ImageButton
        android:id="@+id/feedback"
        android:layout_width="308dp"
        android:layout_height="77dp"
        android:layout_marginTop="28dp"
        android:contentDescription="@string/feedback"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contact"
        app:srcCompat="@drawable/feedbackbuttons" />

    <ImageButton
        android:id="@+id/donatebutton"
        android:layout_width="308dp"
        android:layout_height="77dp"
        android:layout_marginTop="28dp"
        android:contentDescription="@string/donatebutton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/schedule"
        app:srcCompat="@drawable/donatebuttons" />

    <ImageButton
        android:id="@+id/contact"
        android:layout_width="308dp"
        android:layout_height="77dp"
        android:layout_marginTop="28dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/donatebutton"
        app:srcCompat="@drawable/contactbuttons"
        android:contentDescription="@string/contact" />

</androidx.constraintlayout.widget.ConstraintLayout>